{
  "name": "@jagzmz/json-to-cypher",
  "version": "0.0.2",
  "description": "Convert JSON to Cypher queries",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "assets",
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup --tsconfig tsconfig.app.json",
    "prepublishOnly": "pnpm run build",
    "publish": "npm publish",
    "postpublish": "rm *.tgz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jsonpath-plus": "^10.3.0",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "neo4j-driver": "^5.28.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
